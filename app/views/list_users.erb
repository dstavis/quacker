  <% if @users %>
  <h2>All Quacker Users </h2>
  <ul>
    <% @users.each do |user| %>
      <li><%= user.username %>
        <form action="/flockings/new?followee_id=<%=user.id%>" method="post">
        <input type="submit" value="add to flock">
        </form>

      </li>
    <% end %>
  </ul>
  <% end %>
