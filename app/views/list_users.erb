  <% if @users %>
  <ul>
    <% @users.each do |user| %>
      <li><h4><%= user.username %></h4><%= user.bio %>
        <form action="/flockings/new?followee_id=<%=user.id%>" method="post">
        <input type="submit" value="Imprint on this duck">
        </form>

      </li>
    <% end %>
  </ul>
  <% end %>
